{
    "name": "edwrodrig/file_cache",
    "description": "Library to cache images and files for websites",
    "authors": [
        {
            "name": "Edwin Rodríguez León",
            "email": "edwrodrig@gmail.com",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "keywords" : [
        "file cache manager"
    ],
    "license" : "MIT",
    "minimum-stability" : "stable",
    "suggest" : {
        "edwrodrig/contento" : "To manage types and collections",
        "edwrodrig/google_utils" : "To interact with Google Drive data"
    },
    "require": {
        "php" : "^7.4",
        "ext-imagick": "*",
        "ext-json": "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^8.5",
        "edwrodrig/image" : "^2.0",
        "edwrodrig/contento" : "dev-master",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload" : {
        "psr-4" : {
            "edwrodrig\\file_cache\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "test\\edwrodrig\\file_cache\\" : "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/tests",
            ".git",
            ".gitignore",
            "/.travis.yml",
            "/phpunit.xml.dist",
            "/examples",
            "/.idea",
            "README.md"
        ]
    }
}
